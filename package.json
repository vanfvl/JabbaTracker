{
  "name": "jabba-tracker",
  "version": "1.0.0",
  "description": "Time keeping app",
  "main": "index.js",
  "repository": "git@github.com:vanfvl/JabbaTracker.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base public/ --port 8081",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "build": "webpack -p",
    "deploy": "webpack -p && firebase deploy",
    "backup": "firebase database:get /users > backup/users-${DATE}.json && firebase database:get /entries > backup/entries-${DATE}.json && firebase database:get /accounts > backup/accounts-${DATE}.json && firebase auth:export backup/auth-${DATE}.json"
  },
  "author": "Vanni Lammoglia",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.6.0",
    "tailwindcss": "^3.0.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "babel-preset-stage-1": "^6.1.18",
    "firebase": "^4.3.0",
    "fixed-data-table-2": "^0.8.0",
    "lodash": "^3.10.1",
    "material-ui": "^0.19.0",
    "material-ui-icons": "^1.0.0-beta.5",
    "moment": "^2.18.1",
    "react": "^16.8.0",
    "react-autocomplete": "^1.7.1",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^16.8.0",
    "react-redux": "5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.0.4",
    "styled-jsx": "^1.0.10",
    "zipcelx": "^1.2.0"
  }
}
